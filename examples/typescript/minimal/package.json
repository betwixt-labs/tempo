{
  "name": "@examples/minimal",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build:server": "bebopc &&  tsup ./server/index.ts --out-dir ./dist/server/",
    "dev:server": "bebopc && tsx watch server",
    "dev:client": "bebopc && wait-port 3000 && tsx watch client",
    "dev:browser": "yarn webpack serve --mode development --open --hot",
    "start:server": "bebopc && node ./dist/server/index.cjs",
    "build:browser": "webpack --mode production"
  },
  "workspaces": [
    "client",
    "server",
    "shared",
    "browser"
  ],
  "devDependencies": {
    "@types/node": "^18.7.20",
    "bebop-tools": "^2.6.2",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^4.8.3",
    "wait-port": "^1.0.4",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.1",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "dependencies": {
    "@tempojs/client": "^0.0.1-alpha.19",
    "@tempojs/node-http-router": "^0.0.1-alpha.19",
    "bebop": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
